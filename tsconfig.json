{
    "compilerOptions": {
      "target": "ES2023",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "lib": ["ES2023", "DOM"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }